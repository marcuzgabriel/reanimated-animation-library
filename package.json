{
  "name": "@marcuzgabriel/reanimated-animation-library",
  "author": "Marcuz Gabriel Larsen <marcuzgabriel@gmail.com>",
  "version": "1.0.9",
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test:unit": "jest",
    "test:jest:ci:ios": "yarn jest:rngh:fix:ios && yarn jest --coverage",
    "test:jest:watch:ios": "yarn jest:rngh:fix:ios && yarn jest --watch",
    "test:jest:ci:windows": "yarn jest:rngh:fix:windows && yarn jest --coverage",
    "test:jest:watch:windows": "yarn jest:rngh:fix:windows && yarn jest --watch",
    "format": "prettier --write --list-different ./src/**",
    "lint-diff": "eslint --ext '.js,.ts,.tsx' src/** && yarn prettier --check src/**",
    "web": "cross-env NODE_ENV=development webpack serve --config web/webpack.config.js",
    "web:clean": "rimraf web/vendor/ web/build",
    "web:build": "rimraf web/build/; cross-env NODE_ENV=production webpack --config web/webpack.config.js",
    "type:check": "yarn tsc --noEmit",
    "type:generate:windows": "yarn type:generate:clean && yarn type:generate:create && yarn type:generate:tsc && yarn type:generate:delete:files:windows && yarn type:generate:delete:tests && yarn type:generate:copy:windows",
    "type:generate:ios": "yarn type:generate:clean && yarn type:generate:create && yarn type:generate:tsc && yarn type:generate:delete:files:ios && yarn type:generate:delete:tests && yarn type:generate:copy:ios",
    "type:generate:clean": "cross-env-shell rimraf lib",
    "type:generate:create": "cross-env-shell mkdir lib",
    "type:generate:tsc": "yarn tsc",
    "type:generate:delete:tests": "cross-env-shell rimraf ./lib/worklets/__tests__ && rimraf ./lib/helpers/__tests__",
    "type:generate:delete:files:windows": "cross-env-shell del \"lib\\index.js && cross-env-shell del \"lib\\*.web.js && cross-env-shell del \"lib\\ReanimatedAnimationLibrary.js",
    "type:generate:delete:files:ios": "rm ./lib/index.js && rm ./lib/index.web.js && rm ./lib/ReanimatedAnimationLibrary.js",
    "type:generate:copy:windows": "cross-env-shell Xcopy \"src\\ReanimatedAnimationLibrary.ts\" \"lib\\index.js\" /Y",
    "type:generate:copy:ios": "rsync -auv ./src/ReanimatedAnimationLibrary.ts ./lib/index.js",
    "jest:rngh:fix:windows": "cross-env-shell Xcopy \"src\\utils\\fixRNGHmocks.ts\" \"node_modules\\react-native-gesture-handler\\lib\\commonjs\\mocks.js\" /Y",
    "jest:rngh:fix:ios": "rsync -auv ./src/utils/fixRNGHmocks.ts ./node_modules/react-native-gesture-handler/lib/commonjs/mocks.js"
  },
  "main": "lib/index.js",
  "module": "lib/index",
  "types": "reanimated-animation-library.d.ts",
  "react-native": "src/ReanimatedAnimationLibrary",
  "files": [
    "src/",
    "lib/",
    "reanimated-animation-library.d.ts",
    "README.md",
    "!__tests__",
    "!__snapshots__",
    "!*.test.js",
    "!*.test.ts"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-svg": "*",
    "react-native-svg-web": "*",
    "react-native-reanimated": "*",
    "react-native-redash": "*",
    "style-loader": "*",
    "styled-components": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-arrow-functions": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime": "^7.17.9",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^17.0.43",
    "@types/react-native": "^0.67.4",
    "@types/react-test-renderer": "^18.0.0",
    "@types/setimmediate": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@types/styled-components": "^5.1.25",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "core-js": "^3.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-sonar-reporter": "^2.0.0",
    "metro-react-native-babel-preset": "^0.70.1",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "react": "18.0.0",
    "react-dom": "^18.0.0",
    "react-native": "0.68.1",
    "react-native-gesture-handler": "^2.4.0",
    "react-native-reanimated": "2.7.0",
    "react-native-redash": "^16.2.3",
    "react-native-svg": "12.3.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "0.17.7",
    "react-refresh": "^0.12.0",
    "react-test-renderer": "18.0.0",
    "setimmediate": "^1.0.5",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcuzgabriel/reanimated-animation-library.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry":"https://npm.pkg.github.com/"
  }
}
